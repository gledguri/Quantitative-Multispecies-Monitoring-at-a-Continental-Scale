  author = {Hill, Nicole A. and Barrett, Neville and Lawrence, Emma and Hulls, Justin and Dambacher, Jeffrey M. and Nichol, Scott and Williams, Alan and Hayes, Keith R.},
  editor = {Bailey, David Mark},
  year = {2014},
  journal = {PLoS ONE},
