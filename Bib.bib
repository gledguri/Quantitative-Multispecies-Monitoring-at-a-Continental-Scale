@article{andruszkiewiczallan2021,
  title = {Environmental {{DNA}} Shedding and Decay Rates from Diverse Animal Forms and Thermal Regimes},
  author = {Andruszkiewicz Allan, Elizabeth and Zhang, Weifeng Gordon and C. Lavery, Andone and F. Govindarajan, Annette},
