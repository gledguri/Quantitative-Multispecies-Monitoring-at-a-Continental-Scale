@article{marshall2021,
  title = {Environmental (e){{RNA}} Advances the Reliability of {{eDNA}} by Predicting Its Age},
  author = {Marshall, Nathaniel T. and Vanderploeg, Henry A. and Chaganti, Subba Rao},
  year = {2021},
