@article{ingvaldsen2017,
  title = {Atlantic Cod ({{Gadus}} Morhua) Feeding over Deep Water in the High {{Arctic}}},
  author = {Ingvaldsen, Randi B. and Gjøsæter, Harald and Ona, Egil and Michalsen, Kathrine},
