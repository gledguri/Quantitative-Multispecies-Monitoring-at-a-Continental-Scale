  author = {Novotny, Andreas and Serandour, Baptiste and Kortsch, Susanne and Gauzens, Benoit and Jan, Kinlan M.~G. and Winder, Monika},
  year = {2023},
  journal = {Science Advances},
  shortjournal = {Sci. Adv.},
