  title = {Species Redistribution Creates Unequal Outcomes for Multispecies Fisheries under Projected Climate Change},
  author = {Liu, Owen R. and Ward, Eric J. and Anderson, Sean C. and Andrews, Kelly S. and Barnett, Lewis A.~K. and Brodie, Stephanie and Carroll, Gemma and Fiechter, Jerome and Haltuch, Melissa A. and Harvey, Chris J. and Hazen, Elliott L. and Hernvann, Pierre-Yves and Jacox, Michael and Kaplan, Isaac C. and Matson, Sean and Norman, Karma and Pozo Buil, Mercedes and Selden, Rebecca L. and Shelton, Andrew and Samhouri, Jameal F.},
  year = {2023},
  journal = {Science Advances},
  shortjournal = {Sci. Adv.},
  volume = {9},
  number = {33},
  pages = {eadg5468},
  issn = {2375-2548},
  doi = {10.1126/sciadv.adg5468},
  url = {https://www.science.org/doi/10.1126/sciadv.adg5468},
  urlyear = {2024},
  abstract = {Climate change drives species distribution shifts, affecting the availability of resources people rely upon for food and livelihoods. These impacts are complex, manifest at local scales, and have diverse effects across multiple species. However, for wild capture fisheries, current understanding is dominated by predictions for individual species at coarse spatial scales. We show that species-specific responses to localized environmental changes will alter the collection of co-occurring species within established fishing footprints along the U.S. West Coast. We demonstrate that availability of the most economically valuable, primary target species is highly likely to decline coastwide in response to warming and reduced oxygen concentrations, while availability of the most abundant, secondary target species will potentially increase. A spatial reshuffling of primary and secondary target species suggests regionally heterogeneous opportunities for fishers to adapt by changing where or what they fish. Developing foresight into the collective responses of species at local scales will enable more effective and tangible adaptation pathways for fishing communities.           ,              Climate change will alter the distribution of harvested marine species, creating substantial challenges for fishers and managers.},
  langid = {english},
  file = {/Users/gledguri/Zotero/storage/8UAWUT3N/Liu et al. - 2023 - Species redistribution creates unequal outcomes for multispecies fisheries under projected climate c.pdf}
}

