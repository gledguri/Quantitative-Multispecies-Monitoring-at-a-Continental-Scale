@article{cantera2019,
  title = {Optimizing Environmental {{DNA}} Sampling Effort for Fish Inventories in Tropical Streams and Rivers},
  author = {Cantera, Isabel and Cilleros, Kévin and Valentini, Alice and Cerdan, Axel and Dejean, Tony and Iribar, Amaia and Taberlet, Pierre and Vigouroux, Régis and Brosse, Sébastien},
  year = {2019},
