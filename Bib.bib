  shorttitle = {Environmental},
  author = {Blackman, Rosetta and Couton, Marjorie and Keck, François and Kirschner, Dominik and Carraro, Luca and Cereghetti, Eva and Perrelet, Kilian and Bossart, Raphael and Brantschen, Jeanine and Zhang, Yan and Altermatt, Florian},
