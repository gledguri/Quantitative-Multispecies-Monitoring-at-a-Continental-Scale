@article{dick2023,
  title = {Prey Ration, Temperature, and Predator Species Influence Digestion Rates of Prey {{DNA}} Inferred from {{qPCR}} and Metabarcoding},
  author = {Dick, Cory and Larson, Wesley A. and Karpan, Kirby and Baetscher, Diana S. and Shi, Yue and Sethi, Suresh and Fangue, Nann A. and Henderson, Mark J.},
  year = {2023},
