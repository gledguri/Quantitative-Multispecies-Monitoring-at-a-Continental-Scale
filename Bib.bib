  author = {Allan, Bridie Jean Marie and Ray, Jessica Louise and Tiedemann, Maik and Komyakova, Valeriya and Vikebø, Frode and Skaar, Katrine Sandnes and Stiasny, Martina H. and Folkvord, Arild and Nash, Richard D. M. and Stenevik, Erling Kåre and Kjesbu, Olav Sigurd},
