@article{gold2021,
  title = {{{eDNA}} Metabarcoding as a Biomonitoring Tool for Marine Protected Areas},
  author = {Gold, Zachary and Sprague, Joshua and Kushner, David J. and Marin, Erick Zerecero and Barber, Paul H.},
