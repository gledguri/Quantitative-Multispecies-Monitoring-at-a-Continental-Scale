@article{wang2021,
  title = {Methodology of Fish {{eDNA}} and Its Applications in Ecology and Environment},
  author = {Wang, Shuping and Yan, Zhenguang and Hänfling, Bernd and Zheng, Xin and Wang, Pengyuan and Fan, Juntao and Li, Jianlong},
  year = {2021},
  journal = {Science of the Total Environment},
  volume = {755},
