@article{shelton2022,
  title = {Environmental {{DNA}} Provides Quantitative Estimates of {{Pacific}} Hake Abundance and Distribution in the Open Ocean},
  author = {Shelton, Andrew Olaf and Ramón-Laca, Ana and Wells, Abigail and Clemons, Julia and Chu, Dezhang and Feist, Blake E. and Kelly, Ryan P. and Parker-Stetter, Sandra L. and Thomas, Rebecca and Nichols, Krista M. and Park, Linda},
