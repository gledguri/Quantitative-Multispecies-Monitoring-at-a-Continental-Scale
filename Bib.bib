@article{voelker2020,
  title = {Large‐scale Molecular Barcoding of Prey {{DNA}} Reveals Predictors of Intrapopulation Feeding Diversity in a Marine Predator},
  author = {Voelker, Madelyn R. and Schwarz, Dietmar and Thomas, Austen and Nelson, Benjamin W. and Acevedo‐Gutiérrez, Alejandro},
  year = {2020},
  journal = {Ecology and Evolution},
