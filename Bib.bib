@article{djurhuus2020,
  title = {Environmental {{DNA}} Reveals Seasonal Shifts and Potential Interactions in a Marine Community},
  author = {Djurhuus, Anni and Closek, Collin J. and Kelly, Ryan P. and Pitz, Kathleen J. and Michisaki, Reiko P. and Starks, Hilary A. and Walz, Kristine R. and Andruszkiewicz, Elizabeth A. and Olesin, Emily and Hubbard, Katherine and Montes, Enrique and Otis, Daniel and Muller-Karger, Frank E. and Chavez, Francisco P. and Boehm, Alexandria B. and Breitbart, Mya},
