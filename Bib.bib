@article{swalethorp2023,
  title = {Anchovy Boom and Bust Linked to Trophic Shifts in Larval Diet},
  author = {Swalethorp, Rasmus and Landry, Michael R. and Semmens, Brice X. and Ohman, Mark D. and Aluwihare, Lihini and Chargualaf, Dereka and Thompson, Andrew R.},
  year = {2023},
