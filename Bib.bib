  shorttitle = {Validation of {{Environmental}}},
  author = {Ledger, Kimberly~J. and Hicks, Mary~Beth~Rew and Hurst, Thomas~P. and Larson, Wes and Baetscher, Diana~S.},
  year = {2024},
  journal = {Environmental DNA},
