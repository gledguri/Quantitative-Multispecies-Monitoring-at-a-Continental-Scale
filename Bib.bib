@article{erickson2017,
  title = {Seasonal Trends in {{eDNA}} Detection and Occupancy of Bigheaded Carps},
  author = {Erickson, Richard A. and Merkes, Christopher M. and Jackson, Craig A. and Goforth, Reuben R. and Amberg, Jon J.},
  year = {2017},
  journal = {Journal of Great Lakes Research},
