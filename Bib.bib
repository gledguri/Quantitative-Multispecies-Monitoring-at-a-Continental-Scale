  author = {Ford, Jennifer S. and Pelletier, Nathan L. and Ziegler, Friederike and Scholz, Astrid J. and Tyedmers, Peter H. and Sonesson, Ulf and Kruse, Sarah A. and Silverman, Howard},
  year = {2012},
  journal = {Journal of Industrial Ecology},
  volume = {16},
