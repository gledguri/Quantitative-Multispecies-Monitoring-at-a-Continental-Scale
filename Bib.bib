@article{knutsen2007,
  title = {Population Genetic Structure in the {{North Atlantic Greenland}} Halibut ( {{{\mkbibemph{Reinhardtius}}}}{\mkbibemph{ Hippoglossoides}} ): Influenced by Oceanic Current Systems?},
