@article{kokkoris2021,
  title = {Challenges {{Using Droplet Digital PCR}} for {{Environmental Samples}}},
  author = {Kokkoris, Vasilis and Vukicevich, Eric and Richards, Andrew and Thomsen, Corrina and Hart, Miranda M.},
  year = {2021},
  journal = {Applied Microbiology},
  shortjournal = {Applied Microbiology},
